<nav aria-label="breadcrumb" class="my-3">
	<ol class="breadcrumb">
		{% for item in breadcrumbs %}
			{% if loop.last %}
				<li class="breadcrumb-item active" aria-current="page">{{ item.label }}</li>
			{% else %}
				<li class="breadcrumb-item">
					<a href="{{ item.path }}">{{ item.label }}</a>
				</li>
			{% endif %}
		{% endfor %}
	</ol>
</nav>
